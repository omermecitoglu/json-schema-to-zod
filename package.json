{
  "name": "@omer-x/json-schema-to-zod",
  "version": "1.0.0",
  "description": "Zod schema generator from JSON schema specs",
  "keywords": [
    "json-schema",
    "zod",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omermecitoglu/json-schema-to-zod.git"
  },
  "bugs": {
    "url": "https://github.com/omermecitoglu/json-schema-to-zod/issues"
  },
  "homepage": "https://github.com/omermecitoglu/json-schema-to-zod#readme",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Omer Mecitoglu",
    "email": "omer.mecitoglu@gmail.com",
    "url": "https://omermecitoglu.github.io"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
