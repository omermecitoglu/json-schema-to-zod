    "@omer-x/openapi-types": "^1",
    "zod": "^3"
  },
  "devDependencies": {
    "@omer-x/eslint-config": "^2.1.3",
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
